{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mobilefrontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "locales",
        "assets",
        "favicon.ico",
        "login.html",
        "logout.html",
        "error.html",
        "loginNotSuccessfull.html",
        "unauthenticated.html",
        "forbidden.html",
        "notfound.html",
        "aucobo.css",
        "ng2-toastr.min.css"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "ng2-toastr.min.css"
      ],
      "scripts": [],
      "environmentSource": "app/environments/environment.ts",
      "environments": {
        "dev": "app/environments/environment.ts",
        "prod": "app/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    },
    "serve": {
      "port": 4200,
      "host": "localhost"
    }
  }
}
